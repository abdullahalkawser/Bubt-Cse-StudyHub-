{
  "expo": {
    "name": "BubtCse-Study-Hab",
    "slug": "BubtStudyHab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/4.png",
    "scheme": "bubtstudyhab",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/4.png",
        "backgroundImage": "./assets/images/4.png",
        "monochromeImage": "./assets/images/4.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.BubtStudyHab"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "92db9855-fe3c-4976-b0e1-e2e6aa575c9c"
      }
    }
  }
}
